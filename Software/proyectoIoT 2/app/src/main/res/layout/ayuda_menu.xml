<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ImageView
        android:id="@+id/ivBack3"
        android:layout_width="43dp"
        android:layout_height="36dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bg_circle"
        android:onClick="returnRes"
        android:padding="6dp"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/app_name"
        android:textColor="@color/colorPrimary"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivBack3" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivBack3">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/poppins_bold"
            android:paddingStart="16dp"
            android:text="@string/ayuda"
            android:textColor="@color/colorPrimary"
            android:textSize="20dp" />

        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#5687C8" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/bg_circle"
            android:onClick="terminos"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/button1"
                android:layout_width="315dp"
                android:layout_height="61dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/bg_circle"
                android:fontFamily="@font/poppins_medium"
                android:gravity="center_vertical"

                android:paddingStart="8dp"
                android:text="@string/terminosYCondiciones"
                android:textColor="#000000"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/flecha1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                app:srcCompat="@drawable/mayor" />

        </LinearLayout>

        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#5687C8" />

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/bg_circle"
            android:onClick="politica"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/button2"
                android:layout_width="315dp"
                android:layout_height="61dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/bg_circle"
                android:fontFamily="@font/poppins_medium"
                android:gravity="center_vertical"
                android:paddingStart="8dp"
                android:text="@string/politicaPrivacidad"
                android:textColor="#000000"
                android:textSize="16dp" />

            <ImageView
                android:id="@+id/flecha2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                app:srcCompat="@drawable/mayor" />

        </LinearLayout>

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#5687C8" />

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/bg_circle"
            android:onClick="faq"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/button3"
                android:layout_width="315dp"
                android:layout_height="61dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/bg_circle"
                android:fontFamily="@font/poppins_medium"
                android:gravity="center_vertical"
                android:paddingStart="8dp"
                android:text="@string/preguntasFrecuentes"
                android:textColor="#000000"
                android:textSize="16dp" />

            <ImageView
                android:id="@+id/flecha3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                app:srcCompat="@drawable/mayor" />

        </LinearLayout>

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#5687C8" />

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:background="@drawable/bg_circle"
            android:onClick="lanzarContactos"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/button4"
                android:layout_width="315dp"
                android:layout_height="61dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/bg_circle"
                android:fontFamily="@font/poppins_medium"
                android:gravity="center_vertical"
                android:paddingStart="8dp"

                android:text="@string/contacto"
                android:textColor="#000000"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/flecha4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                app:srcCompat="@drawable/mayor" />

        </LinearLayout>

        <View
            android:id="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="#5687C8" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>